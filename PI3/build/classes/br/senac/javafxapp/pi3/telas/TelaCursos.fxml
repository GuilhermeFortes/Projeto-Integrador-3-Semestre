<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.senac.javafxapp.pi3.telas.TelaCursosController">
   <children>
      <TableView fx:id="TBLCursos" layoutX="64.0" layoutY="120.0" prefHeight="200.0" prefWidth="673.0">
        <columns>
          <TableColumn fx:id="TBCID" prefWidth="57.0" text="ID Curso" />
          <TableColumn fx:id="TBCNome" prefWidth="211.0" text="Nome do Curso" />
            <TableColumn fx:id="TBCDescricao" prefWidth="141.0" text="Descrição" />
            <TableColumn fx:id="TBCCarga" prefWidth="68.0" text="Carga" />
            <TableColumn fx:id="TBCTotaulas" prefWidth="119.0" text="Total de Aulas" />
            <TableColumn fx:id="TBCAno" prefWidth="76.0" text="Ano" />
        </columns>
      </TableView>
      <Button fx:id="BTBack" layoutX="374.0" layoutY="349.0" mnemonicParsing="false" onAction="#Back" text="Voltar" />
      <TextField fx:id="TFSearch" layoutX="232.0" layoutY="83.0" promptText="Procurar Curso" />
      <ComboBox fx:id="TBCSearchType" layoutX="64.0" layoutY="83.0" onAction="#SearchType" prefWidth="150.0" promptText="Tipo" />
      <Button fx:id="BTUpdate" layoutX="395.0" layoutY="83.0" mnemonicParsing="false" onAction="#Update" text="Atualizar" />
   </children>
</AnchorPane>
